<?xml version="1.0"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>

  <appSettings>
    <!--打印机状态接口-->
    <add key="LiveReportAPI" value="http://printer.wiwibao.com/Home/Printaction/live"/>
    <!--获取订单信息接口-->
    <add key="GetOrderAPI" value="http://printer.wiwibao.com/Home/Printaction/get_order"/>
    <!--打印结果接口-->
    <add key="PrintResultAPI" value="http://printer.wiwibao.com/Home/Printaction/print_result"/>
    <!--取件成功报告接口-->
    <add key="PickSuccessAPI" value="http://printer1.wiwibao.com/Home/Printaction/get_success"/>
    <!--是否取件成功报告接口-->
    <add key="IsPickSuccessAPI" value="http://printer1.wiwibao.com/Home/Printaction/is_get_success"/>
    <!--状态报告间隔 2*60*1000 = 120000 2分钟-->
    <add key="StateReportTimeSpan" value="120000"/>
    <!--验证码 最长位数-->
    <add key="MaxInputLength" value="6"/>
  </appSettings>
</configuration>
